let money=parseInt(prompt('Сколько у вас денег?'))
let appl=parseInt(prompt('Сколько вы купили яблок?'))
let bread=parseInt(prompt('Сколько вы купили хлеба?'))
let appl_1=parseInt(prompt('Сколько стоит одно яблоко?'))
let AllAppl=appl_1*appl
let bread1=parseInt(prompt('Сколько стоит батон хлеба?'))
let AllBread=bread1*bread
let Basket=AllAppl+AllBread
let result=money>=Basket
document.body.innerHTML='Хватит денег на покупку? '+result